{
  "name": "steffjenl/laravel-signhost",
  "description": "Laravel package for Signhost",
  "type": "library",
  "keywords": [
    "ondertekenen",
    "signhost",
    "client",
    "laravel",
    "api"
  ],
  "homepage": "http://github.com/steffjenl/laravel-signhost",
  "license": "MIT",
  "authors": [
    {
      "name":  "StÃ¨phan Eizinga",
      "email": "stephan.eizinga@gmail.com",
      "role":  "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": ">=5.4.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "php": ">=7.1.3",
    "orchestra/testbench": "^3.5",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0",
    "squizlabs/php_codesniffer": "^3.1",
    "psy/psysh": "^0.8.13"
  },
  "suggest": {
    "ext-curl": "Allows usage of cURL transport adapter"
  },
  "autoload": {
    "psr-4": {
      "Signhost\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Signhost\\SignhostServiceProvider"
      ],
      "aliases": {
        "Signhost": "Signhost\\SignhostFacade"
      }
    }
  }
}